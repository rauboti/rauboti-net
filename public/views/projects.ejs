<!DOCTYPE html>
<html>
  <head>
    <title><%=title%></title>
    <!-- Cleaner font -->
    <link href="https://fonts.googleapis.com/css?family=Rajdhani" rel="stylesheet">
    <!-- CSS source files -->
    <link rel="stylesheet" href="/css/main.css" />
    <link rel="stylesheet" href="/css/boti.css" />
    <nav class="nav-rauboti">
      <menu class="menu-rauboti">
        <% for(var i=0; i<menu.length; i++) {
          if (menu[i].id === activePage) {%>
          <a href="<%=menu[i].path%>"><div id="<%=menu[i].id%>" class="menu-button-big menu-button-selected"><div class="menu-button-text"><%=menu[i].text%></div></div></a>
        <%} else {%>
          <a href="<%=menu[i].path%>"><div id="<%=menu[i].id%>" class="menu-button-big"><div class="menu-button-text"><%=menu[i].text%></div></div></a>
        <%}}%>
      </menu>
    </nav>
  </head>
  <body>
    <section id="pageContainer">
      <div id="cntrCode" class="container-rauboti">
        <h1># Project list</h1>
        <% for (var i = 0; i < projects.length; i++) {%>
          <a href="<%=projects[i].link%>" target="_blank"><div class="container-rauboti-group project-box"><h2><%=projects[i].name%></h2><div class="project-description"><%=projects[i].description%></div></div></a>
        <%}%>
      </div>
    </section>
  </body>
  <foot>
    <!-- JS source files -->
    <script src="/js/jquery-3.3.1.min.js"></script>
    <script src="/js/main.js"></script>
  </foot>
</html>
